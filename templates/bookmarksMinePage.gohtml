{{ template "head" $ }}
    <table>
        <tr valign="top">
            {{- range bookmarkColumns }}
                <td>
                    {{- range .Categories }}
                        <ul>
                            <h2>{{ .Name }}</h2>
                            {{- range .Entries }}
                                <li><a href="{{ .Url }}" target="_blank">{{ .Name }}</a>
                            {{- end }}
                        </ul>
                    {{- end }}
                </td>
            {{- end }}
        </tr>
    </table>
    <hr><a href="/bookmarks/edit">EDIT</a><br>
{{ template "tail" $ }}